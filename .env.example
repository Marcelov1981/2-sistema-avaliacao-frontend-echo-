# Configuração da API do Google Gemini (LEGACY - Mantido para compatibilidade)
# Obtenha sua chave em: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Configuração da IA Personalizada GeoMind - APIs de Visão Computacional
# OpenAI GPT-4 Vision (Recomendado)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude Vision
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Vision API
VITE_GOOGLE_VISION_KEY=your_google_vision_api_key_here

# Azure Computer Vision
VITE_AZURE_VISION_KEY=your_azure_vision_key_here
VITE_AZURE_VISION_ENDPOINT=your_azure_vision_endpoint_here

# APIs de Geolocalização
# OpenCage Geocoding (Recomendado para geolocalização reversa)
VITE_OPENCAGE_API_KEY=your_opencage_api_key_here

# Mapbox (Alternativa para geolocalização)
VITE_MAPBOX_API_KEY=your_mapbox_api_key_here

# Google Maps Geocoding (Alternativa)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Configuração da API Backend para Produção
# Configure a URL do seu backend em produção
VITE_API_BASE_URL=https://your-backend-url.com/api/v1

# Exemplos de configuração:
# VITE_OPENAI_API_KEY=sk-proj-abc123...
# VITE_ANTHROPIC_API_KEY=sk-ant-api03-abc123...
# VITE_GOOGLE_VISION_KEY=AIzaSyCc3gYZ6IYcJxdLAQJqa8fDMVc2uptAhTg
# VITE_OPENCAGE_API_KEY=abc123def456...
# VITE_API_BASE_URL=https://my-backend.railway.app/api/v1